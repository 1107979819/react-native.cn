{
  "useCommand": true,
  "defaultCommand": "development",
  "commands": {
    "production":{
      "options": {
        "production": {
          "default": true
        },
        "launchServer":{
          "default": true
        }
      }
    },
    "productionWithAssets":{
      "options": {
        "production": {
          "default": true
        },
        "launchServer":{
          "default": true
        },
        "serveAssets":{
          "default": true
        }
      }
    },
    "development":{
      "options": {
        "launchServer":{
          "default": true
        }
      }
    },
    "developmentWithWebpack":{
      "options": {
        "serveWebpack":{
          "default": false
        },
        "launchServer":{
          "default": true
        }
      }
    },
    "build-client":{
      "options": {
        "production": {
          "default": true
        }
      }
    },
    "webpack-dev":{
      "options": {
        "serveWebpack":{
          "default": true
        }
      }
    }
  },
  "globalOptions":{
    "production": {
      "default": false
    },
    "enableSSR":{
      "default": true
    },
    "port": {
      "default": 3000,
      "hasValue": true,
      "type": "int"
    },
    "host": {
      "default": "localhost",
      "hasValue": true
    },
    "webpackPort": {
      "default": 3001,
      "hasValue": true,
      "type": "int"
    },
    "webpackHost": {
      "default": "localhost",
      "hasValue": true
    },
    "serveWebpack":{
      "default": false
    },
    "launchServer":{
      "default": false
    },
    "serveAssets": {
      "default": false
    },
    "showDevTool":{
      "default": false
    }
  }
}